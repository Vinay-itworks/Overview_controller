<span class="badge text-bg-secondary" >
  User Created : <%= time_ago_in_words(@user.created_at) %> | 
  User Updated : <%= time_ago_in_words(@user.updated_at) %>
</span>
<hr/>
<div id="<%= dom_id user %>">
  <p>
    <strong>First name:</strong>
    <%= user.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Password:</strong>
    <%= user.password %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Contact:</strong>
    <%= number_to_phone(user.contact) %>
  </p>

  <p>
    <strong>Current address:</strong>
    <%= user.current_address %>
  </p>

</div>
